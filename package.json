{
	"name": "typescript-nodejs",
	"version": "1.0.0",
	"description": "This is structure project of typescript application, develop by DS112",
	"main": "www",
	"scripts": {
		"build": "echo \"Using webpack build\" && exit 1",
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "tsc && node dist/index.js",
		"dev": "concurrently \"nodemon --exec ts-node ./src/bin/www.ts\" \"nodemon --exec ts-node ./src/worker/start.worker.ts\"",
		"worker:dev": "nodemon --exec ts-node ./src/worker/start.worker.ts",
		"webpack-build-api": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.api.ts",
		"webpack-watch-api": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.api.ts --watch",
		"watch-build-api": "nodemon \"./dist/api/index.js\" --watch \"./dist\" ",
		"webpack-build-worker": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.worker.ts",
		"webpack-watch-worker": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.worker.ts --watch",
		"watch-build-worker": "nodemon \"./dist/worker/index.js\" --watch \"./dist\" ",
		"webpack-start-all": "concurrently \"nodemon \"./dist/worker/index.js\" --watch \"./dist\" \"  \"nodemon \"./dist/api/index.js\" --watch \"./dist\" \""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/onepiecehung/typescript-nodejs.git"
	},
	"keywords": [
		"typescript",
		"node"
	],
	"author": "DS112, ONEPIECEHUNG",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/onepiecehung/typescript-nodejs/issues"
	},
	"homepage": "https://github.com/onepiecehung/typescript-nodejs#readme",
	"dependencies": {
		"@types/amqplib": "^0.5.16",
		"@types/bcrypt": "^3.0.0",
		"@types/clean-webpack-plugin": "^0.1.3",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.8",
		"@types/debug": "^4.1.5",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.8",
		"@types/http-errors": "^1.8.0",
		"@types/ioredis": "^4.17.7",
		"@types/joi": "^14.3.4",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mongoose-paginate-v2": "^1.3.8",
